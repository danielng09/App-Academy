<h1> Sign In! </h1>

<form action="<%= session_url %>" method="POST">
  <label for="session_email">Email</label>
  <input type="text"
         name="session[email]"
         id="session_email">
  <br>
  <label for="session_password">Password</label>
  <input type="password"
         name="session[password]"
         id="session_password">
<br>
  <input type="submit" value="Sign In!">
</form>
