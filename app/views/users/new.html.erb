<h1> Create a new user! </h1>


<form action="<%= users_url %>" method="POST">
  <label for="user_email">Email</label>
  <input type="text"
         name="user[email]"
         id="user_email"
         value="<%= @user.email %>">
  <br>
  <label for="user_password">Password</label>
  <input type="password"
         name="user[password]"
         id="user_password">
<br>
  <input type="submit" value="Create account">
</form>
